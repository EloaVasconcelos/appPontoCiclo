        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,VisualLintOverlap,VisualLintButtonSize" />

    <Button
        android:id="@+id/btnreci"
        android:layout_width="384dp"
        android:layout_height="76dp"
        android:layout_marginStart="170dp"
        android:layout_marginTop="432dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="315dp"
        android:backgroundTint="#00bf63"
        android:text="@string/btnreci"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btnsombra2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,TextSizeCheck,VisualLintButtonSize" />

    <Button
        android:id="@+id/btnpontos"
        android:layout_width="384dp"
        android:layout_height="76dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="150dp"
        android:backgroundTint="#8300BF63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnsombra2"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="DuplicateIds,MissingConstraints,SpeakableTextPresentCheck,VisualLintButtonSize,VisualLintOverlap" />

    <Button
        android:id="@+id/btnpontos"
        android:layout_width="388dp"
        android:layout_height="79dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="160dp"
        android:backgroundTint="#00bf63"
