        app:layout_constraintTop_toBottomOf="@+id/btnsombra2"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="DuplicateIds,HardcodedText,MissingConstraints,TextContrastCheck,TextSizeCheck,VisualLintButtonSize,VisualLintBounds" />

    <ImageView
        android:id="@+id/imglogo2"
        android:layout_width="233dp"
        android:layout_height="195dp"
        android:layout_marginStart="89dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="593dp"
        android:contentDescription="@string/imglogo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo"
        tools:ignore="ImageContrastCheck" />

    <Button
        android:id="@+id/btnreci1"
        android:layout_width="382dp"
        android:layout_height="67dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="385dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="279dp"
        android:backgroundTint="#8300BF63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnreci"
        android:layout_width="383dp"
        android:layout_height="82dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="291dp"
        android:backgroundTint="#00bf63"
        android:text="@string/btnreci"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.559" />


</androidx.constraintlayout.widget.ConstraintLayout>