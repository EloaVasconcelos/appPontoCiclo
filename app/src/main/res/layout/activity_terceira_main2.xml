<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tela3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#070101"
    tools:context=".TerceiraMainActivity2">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="415dp"
        android:layout_height="32dp"
        android:layout_marginTop="125dp"
        android:background="#5CEC62"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnpontos"
        tools:ignore="MissingConstraints,VisualLintBounds"></LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="415dp"
        android:layout_height="32dp"
        android:layout_marginBottom="3dp"
        android:background="#5CEC62"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/imglogo2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="DuplicateIds,MissingConstraints,VisualLintBounds"></LinearLayout>


    <Button
        android:id="@+id/btnsombra1"
        android:layout_width="355dp"
        android:layout_height="76dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="425dp"
        android:backgroundTint="#8300BF63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imglogo2"
        app:layout_constraintVertical_bias="0.181"
        tools:ignore="MissingConstraints,VisualLintOverlap,SpeakableTextPresentCheck,VisualLintButtonSize" />

    <Button
        android:id="@+id/btnsus"
        android:layout_width="355dp"
        android:layout_height="75dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="395dp"
        android:backgroundTint="#00bf63"
        android:text="@string/sustentabilidade"
        android:textColor="#FFFFFFFF"
        android:textSize="31sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imglogo2"
        app:layout_constraintVertical_bias="0.445"
        tools:ignore="MissingConstraints,TextSizeCheck,TextContrastCheck,VisualLintButtonSize,VisualLintOverlap" />

    <Button
        android:id="@+id/btnsombra3"
        android:layout_width="355dp"
        android:layout_height="75dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="135dp"
        android:backgroundTint="#8300BF63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnrecicla"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="DuplicateIds,MissingConstraints,SpeakableTextPresentCheck,VisualLintButtonSize,VisualLintOverlap" />

    <Button
        android:id="@+id/btnpontos"
        android:layout_width="355dp"
        android:layout_height="75dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="145dp"
        android:backgroundTint="#00bf63"
        android:text="@string/btnpontos"
        android:textColor="#ffffff"
        android:textSize="31sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnrecicla"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="DuplicateIds,HardcodedText,MissingConstraints,TextContrastCheck,TextSizeCheck,VisualLintButtonSize,VisualLintBounds" />

    <ImageView
        android:id="@+id/imglogo2"
        android:layout_width="233dp"
        android:layout_height="195dp"
        android:layout_marginStart="89dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="593dp"
        android:contentDescription="@string/imglogo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo"
        tools:ignore="ImageContrastCheck" />

    <Button
        android:id="@+id/btnrecicla"
        android:layout_width="355dp"
        android:layout_height="67dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="385dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="225dp"
        android:backgroundTint="#8300BF63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/btnreci"
        android:layout_width="355dp"
        android:layout_height="75dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="260dp"
        android:backgroundTint="#00bf63"
        android:text="@string/btnreci"
        android:textColor="#FFFFFFFF"
        android:textSize="31sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.72" />


</androidx.constraintlayout.widget.ConstraintLayout>