<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tela2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:backgroundTint="#000000"
    tools:context=".SecondMainActivity2">


    <LinearLayout
        android:layout_width="367dp"
        android:layout_height="7dp"
        android:layout_marginTop="437dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="287dp"
        android:background="#5CEC62"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,VisualLintBounds"></LinearLayout>

    <TextView
        android:id="@+id/txttexto3"
        android:layout_width="321dp"
        android:layout_height="118dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="297dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="316dp"
        android:backgroundTint="#000000"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/txttexto3"
        android:textColor="#FFFFFFFF"
        android:textColorLink="#FFFFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck" />

    <TextView
        android:id="@+id/txttexto2"
        android:layout_width="357dp"
        android:layout_height="127dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="75dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/txttexto2"
        android:textColor="#FFFFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/txttexto3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txttexto1"
        app:layout_constraintVertical_bias="0.541"
        tools:ignore="HardcodedText,TextSizeCheck" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="415dp"
        android:layout_height="32dp"
        android:layout_marginTop="-55dp"
        android:layout_marginBottom="699dp"
        android:background="#5CEC62"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,VisualLintBounds"></LinearLayout>

    <LinearLayout
        android:layout_width="415dp"
        android:layout_height="32dp"
        android:layout_marginTop="660dp"
        android:background="#5CEC62"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints,VisualLintBounds" />

    <TextView
        android:id="@+id/txttexto1"
        android:layout_width="236dp"
        android:layout_height="43dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="644dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/txttexto1"
        android:textColor="#19EC22"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck" />

    <TextView
        android:id="@+id/txttexto4"
        android:layout_width="374dp"
        android:layout_height="121dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="470dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="140dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/txttexto3"
        android:textColor="#FFFFFFFF"
        android:textColorLink="#FFFFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck" />

    <LinearLayout
        android:layout_width="367dp"
        android:layout_height="7dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="270dp"
        android:layout_marginBottom="454dp"
        android:background="#5CEC62"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,VisualLintBounds"></LinearLayout>

    <Button
        android:id="@+id/btnproximo"
        android:layout_width="181dp"
        android:layout_height="62dp"
        android:layout_marginStart="209dp"
        android:layout_marginTop="619dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="50dp"
        android:layout_weight="1"
        android:backgroundTint="#00bf63"
        android:text="@string/btnproximo"
        android:textColor="#FFFFFFFF"
        android:textColorHighlight="#FFFFFFFF"
        android:textColorLink="#FFFFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,InefficientWeight,TextContrastCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>